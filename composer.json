{
    "name": "ixudra/imageable",
    "description": "Custom Laravel imaging package for the Laravel 5 framework - developed by Ixudra",
    "version": "7.0.0",
    "keywords": ["Ixudra", "image", "Laravel"],
    "homepage": "http://ixudra.be",
    "license": "MIT",

    "authors": [
        {
            "name": "Jan Oris",
            "email": "jan.oris@ixudra.be"
        }
    ],

    "require": {
        "php": ">=7.0",
        "illuminate/support": "~5.5",
        "illuminate/database": "~5.5",
        "laravelcollective/html": "~5.2",
        "laracasts/presenter": "0.2.*",
        "ixudra/core": "~6.0"
    },

    "suggest": {
        "ixudra/translation": "Custom PHP translation library for the Laravel 5 framework"
    },

    "autoload": {
        "classmap": [
            "src/database/migrations"
        ],
        "psr-4": {
            "Ixudra\\Imageable\\": "src/"
        }
    },

    "extra": {
        "laravel": {
            "providers": [
                "Ixudra\\Imageable\\ImageableServiceProvider"
            ],
            "aliases": {
                "Imageable": "Ixudra\\Imageable\\Imageable"
            }
        }
    }
}
